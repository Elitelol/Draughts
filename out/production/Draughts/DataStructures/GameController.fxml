<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane onMouseClicked="#setCoordinates" prefHeight="950.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="DataStructures.GameController">
   <children>
      <StackPane fx:id="stackPane" prefHeight="800.0" prefWidth="800.0" />
      <Button fx:id="resetCoordinates" layoutX="123.0" layoutY="871.0" mnemonicParsing="false" onAction="#clearCoordinates" text="Reset Coordinates" />
      <Button layoutX="348.0" layoutY="871.0" mnemonicParsing="false" onAction="#setGameOver" text="Surrender" />
      <Label fx:id="turnLabel" layoutX="549.0" layoutY="875.0" text="Label">
         <font>
            <Font size="24.0" />
         </font></Label>
   </children>
</AnchorPane>
